{
  "displayName": "[Video Utils] - Stitching",
  "name": "video-utils-stitching",
  "description": "Video Utils - Stitching is a collection of tools for video manipulation. It includes tools for merging frames into a video and merging videos into a single video.",
  "version": "1.1.3",
  "attributes": {
    "Provider": "Dataloop",
    "Category": "Application",
    "Media Type": [
      "Video",
      "Image"
    ],
    "Application Type": [
      "Pipeline Node"
    ],
    "License": "Apache 2.0"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/video-utils.git",
    "gitTag": "1.1.3"
  },
  "scope": "project",
  "components": {
    "pipelineNodes": [
      {
        "scope": "node",
        "name": "frames-to-video",
        "displayName": "Frames to Video Merge",
        "description": "Merge frames into a video",
        "invoke": {
          "type": "function",
          "namespace": "frames-to-video.frames-to-video.frames_to_vid"
        },
        "categories": [
          "video-utils"
        ],
        "configuration": {
          "fields": [
            {
              "name": "output_video_type",
              "title": "Output Video Type",
              "props": {
                "type": "string",
                "required": true,
                "options": [
                  {
                    "label": "Webm",
                    "value": "webm"
                  },
                  {
                    "label": "MP4",
                    "value": "mp4"
                  }
                ],
                "default": "webm"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-select"
            },
            {
              "name": "tracker",
              "title": "Tracker",
              "props": {
                "type": "string",
                "required": true,
                "options": [
                  {
                    "label": "ByteTrack",
                    "value": "ByteTrack"
                  },
                  {
                    "label": "DeepSORT",
                    "value": "DeepSORT"
                  }
                ],
                "default": "ByteTrack"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-select"
            },
            {
              "name": "output_dir",
              "title": "Dataset Output Directory",
              "props": {
                "title": true,
                "type": "string",
                "default": "output",
                "required": true,
                "placeholder": "Insert output directory"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "input_dir",
              "title": "Dataset Input Directory",
              "props": {
                "title": true,
                "type": "string",
                "default": "input",
                "required": true,
                "placeholder": "Insert input directory"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "fps",
              "title": "FPS",
              "props": {
                "type": "number",
                "default": 30
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      },
      {
        "scope": "node",
        "name": "videos-to-video",
        "displayName": "Videos to Video Merge",
        "description": "Merge videos into a single video",
        "invoke": {
          "type": "function",
          "namespace": "videos-to-video.videos-to-video.videos_to_video"
        },
        "categories": [
          "video-utils"
        ],
        "configuration": {
          "fields": [
            {
              "name": "tracker",
              "title": "Tracker",
              "props": {
                "type": "string",
                "required": true,
                "options": [
                  {
                    "label": "ByteTrack",
                    "value": "ByteTrack"
                  },
                  {
                    "label": "DeepSORT",
                    "value": "DeepSORT"
                  }
                ],
                "default": "ByteTrack"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-select"
            },
            {
              "name": "output_dir",
              "title": "Dataset Output Directory",
              "props": {
                "title": true,
                "type": "string",
                "default": "output",
                "required": true,
                "placeholder": "Insert output directory"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "input_dir",
              "title": "Dataset Input Directory",
              "props": {
                "title": true,
                "type": "string",
                "default": "input",
                "required": true,
                "placeholder": "Insert input directory"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      }
    ],
    "modules": [
      {
        "name": "frames-to-video",
        "entryPoint": "frames_to_video.py",
        "className": "ServiceRunner",
        "initInputs": [],
        "functions": [
          {
            "name": "frames_to_vid",
            "description": "frames to video tool",
            "input": [
              {
                "type": "Item[]",
                "name": "items"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "displayIcon": "qa-sampling",
            "displayName": "Frames to Video Merge"
          }
        ]
      },
      {
        "name": "videos-to-video",
        "entryPoint": "videos_to_video.py",
        "className": "ServiceRunner",
        "initInputs": [],
        "functions": [
          {
            "name": "videos_to_video",
            "description": "videos to video tool",
            "input": [
              {
                "type": "Item[]",
                "name": "items"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "displayIcon": "qa-sampling",
            "displayName": "Videos to Video Merge"
          }
        ]
      }
    ],
    "services": [
      {
        "name": "frames-to-video",
        "moduleName": "frames-to-video",
        "runtime": {
          "podType": "regular-xs",
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/video-utils:0.0.1",
          "numReplicas": 1,
          "concurrency": 10,
          "pyPackages": {},
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 10
          },
          "preemptible": false
        },
        "maxAttempts": 3,
        "executionTimeout": 172800,
        "drainTime": 600,
        "onReset": "failed",
        "runExecutionAsProcess": false,
        "operation": "none"
      },
      {
        "name": "videos-to-video",
        "moduleName": "videos-to-video",
        "runtime": {
          "podType": "regular-xs",
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/video-utils:0.0.1",
          "numReplicas": 1,
          "concurrency": 10,
          "pyPackages": {},
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 10
          },
          "preemptible": false
        },
        "maxAttempts": 3,
        "executionTimeout": 172800,
        "drainTime": 600,
        "onReset": "failed",
        "runExecutionAsProcess": false,
        "operation": "none"
      }
    ]
  }
}