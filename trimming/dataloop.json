{
  "displayName": "[Video Utils] - Trimming",
  "name": "video-utils-trimming",
  "description": "Video Utils - Trimming is a collection of functions for video manipulation. The package includes functions for video splitting, video to videos splitting, and smart subsampling.",
  "version": "1.1.2",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/video-utils.git",
    "gitTag": "1.1.2"
  },
  "scope": "public",
  "attributes": {
    "Category": "Application",
    "Hub": "Dataloop",
    "Provider": "Dataloop",
    "Deployed By": "Dataloop",
    "Application Type": "Pipeline Node",
    "Media Type": [
      "Video",
      "Image"
    ]
  },
  "components": {
    "pipelineNodes": [
      {
        "scope": "node",
        "name": "video-to-frames",
        "invoke": {
          "type": "function",
          "namespace": "video-to-frames.video-to-frames.video_to_frames"
        },
        "categories": [
          "video-utils"
        ],
        "configuration": {
          "fields": [
            {
              "name": "output_folder",
              "title": "Output Folder",
              "props": {
                "title": true,
                "type": "text",
                "required": false,
                "default": "/"
              },
              "widget": "dl-input"
            },
            {
              "name": "mode",
              "title": "Trimming by Mode",
              "props": {
                "type": "text",
                "required": true,
                "options": [
                  {
                    "label": "Frames Interval",
                    "value": "frames_interval"
                  },
                  {
                    "label": "Number of Splits",
                    "value": "num_splits"
                  },
                  {
                    "label": "Seconds of Interval",
                    "value": "seconds_interval"
                  }
                ],
                "default": "frames_interval"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-select"
            },
            {
              "name": "splitter_arg",
              "title": "Splitter Argument",
              "props": {
                "redAsterisk": true,
                "title": true,
                "type": "number",
                "required": true,
                "default": 100
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      },
      {
        "scope": "node",
        "name": "video-to-videos",
        "invoke": {
          "type": "function",
          "namespace": "video-to-videos.video-to-videos.video_to_videos"
        },
        "categories": [
          "video-utils"
        ],
        "configuration": {
          "fields": [
            {
              "name": "output_folder",
              "title": "Output Folder",
              "props": {
                "title": true,
                "type": "text",
                "required": false,
                "default": "/"
              },
              "widget": "dl-input"
            },
            {
              "name": "mode",
              "title": "Trimming by Mode",
              "props": {
                "type": "text",
                "required": true,
                "options": [
                  {
                    "label": "Number of Frames",
                    "value": "num_frames"
                  },
                  {
                    "label": "Number of Splits",
                    "value": "num_splits"
                  },
                  {
                    "label": "Length by Seconds",
                    "value": "out_length"
                  }
                ],
                "default": "num_frames"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-select"
            },
            {
              "name": "splitter_arg",
              "title": "Splitter Argument",
              "props": {
                "redAsterisk": true,
                "title": true,
                "type": "number",
                "required": true,
                "default": 100
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "n_overlap",
              "title": "Number of Overlapping Frames",
              "props": {
                "title": true,
                "type": "number",
                "required": false,
                "default": 0
              },
              "widget": "dl-input"
            }
          ]
        }
      },
      {
        "scope": "node",
        "name": "smart-subsampling",
        "invoke": {
          "type": "function",
          "namespace": "smart-subsampling.smart-subsampling.video_to_frames_smart_subsampling"
        },
        "categories": [
          "video-utils"
        ],
        "configuration": {
          "fields": [
            {
              "name": "output_folder",
              "title": "Output Folder",
              "props": {
                "title": true,
                "type": "text",
                "required": false,
                "default": "/"
              },
              "widget": "dl-input"
            },
            {
              "name": "threshold",
              "title": "Threshold",
              "props": {
                "redAsterisk": true,
                "title": true,
                "type": "number",
                "required": true,
                "default": 0.5
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            },
            {
              "name": "window_size",
              "title": "Window Size",
              "props": {
                "redAsterisk": true,
                "title": true,
                "type": "number",
                "required": true,
                "default": 1
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      }
    ],
    "modules": [
      {
        "name": "video-to-frames",
        "entryPoint": "video_to_frames/video_to_frames.py",
        "className": "ServiceRunner",
        "initInputs": [],
        "functions": [
          {
            "name": "video_to_frames",
            "description": "video splitting tool",
            "input": [
              {
                "type": "Item",
                "name": "item"
              },
              {
                "type": "String",
                "name": "output_folder"
              },
              {
                "type": "String",
                "name": "mode"
              },
              {
                "type": "Integer",
                "name": "splitter_arg"
              }
            ],
            "output": [
